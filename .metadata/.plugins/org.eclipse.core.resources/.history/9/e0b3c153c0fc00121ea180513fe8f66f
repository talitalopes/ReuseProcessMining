<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:rdl="http://www.example.org/po" targetNamespace="http://www.example.org/po">
	<xsd:element name="process" type="rdl:RDLProcess"/>
	<xsd:complexType name="Process">
		<xsd:sequence>
			<xsd:element name="property" type="rdl:Property" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Property">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:string"/>
			<xsd:element name="name" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>